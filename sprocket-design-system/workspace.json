{
  "version": 2,
  "projects": {
    "angular-example-site": {
      "projectType": "application",
      "root": "apps/angular-example-site",
      "sourceRoot": "apps/angular-example-site/src",
      "prefix": "sprocket-design-system",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular-example-site",
            "index": "apps/angular-example-site/src/index.html",
            "main": "apps/angular-example-site/src/main.ts",
            "polyfills": "apps/angular-example-site/src/polyfills.ts",
            "tsConfig": "apps/angular-example-site/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/angular-example-site/src/favicon.ico",
              "apps/angular-example-site/src/assets"
            ],
            "styles": ["apps/angular-example-site/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular-example-site/src/environments/environment.ts",
                  "with": "apps/angular-example-site/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-example-site:build:production"
            },
            "development": {
              "browserTarget": "angular-example-site:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-example-site:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/angular-example-site/src/**/*.ts",
              "apps/angular-example-site/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular-example-site"],
          "options": {
            "jestConfig": "apps/angular-example-site/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "angular-example-site-e2e": {
      "root": "apps/angular-example-site-e2e",
      "sourceRoot": "apps/angular-example-site-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-example-site-e2e/cypress.json",
            "devServerTarget": "angular-example-site:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-example-site:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/angular-example-site-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "component-library": {
      "root": "apps/component-library",
      "sourceRoot": "apps/component-library/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/component-library",
            "index": "apps/component-library/src/index.html",
            "baseHref": "/",
            "main": "apps/component-library/src/main.tsx",
            "polyfills": "apps/component-library/src/polyfills.ts",
            "tsConfig": "apps/component-library/tsconfig.app.json",
            "assets": [
              "apps/component-library/src/favicon.ico",
              "apps/component-library/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/component-library/src/environments/environment.ts",
                  "with": "apps/component-library/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "component-library:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "component-library:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/component-library/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/component-library"],
          "options": {
            "jestConfig": "apps/component-library/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/component-library/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/component-library",
            "config": {
              "configFolder": "apps/component-library/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "component-library-e2e": {
      "root": "apps/component-library-e2e",
      "sourceRoot": "apps/component-library-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/component-library-e2e/cypress.json",
            "devServerTarget": "component-library:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "component-library:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
