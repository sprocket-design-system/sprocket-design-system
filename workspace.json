{
  "version": 2,
  "projects": {
    "angular-example-site": "apps/angular-example-site",
    "angular-example-site-e2e": "apps/angular-example-site-e2e",
    "component-library": "apps/component-library",
    "component-library-e2e": "apps/component-library-e2e",
    "design-tokens": "libs/design-tokens",
    "react-example-site": "apps/react-example-site",
    "react-example-site-e2e": "apps/react-example-site-e2e",
    "reference-site": "apps/reference-site",
    "reference-site-e2e": "apps/reference-site-e2e",
    "web-components": "libs/web-components"
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "generators": {
    "@nrwl/web:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "@emotion/styled"
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "component-library"
}
